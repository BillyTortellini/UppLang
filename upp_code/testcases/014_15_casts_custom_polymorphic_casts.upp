
import ~Type_Handle

// Custom casts also work on polymorphic functions
Optional::struct($T:Type_Handle)
	value:T
	available:bool

is_available::(opt:Optional($T))=>bool
	return opt.available

main::()
	context add_cast(is_available,.IMPLICIT)
	value:Optional(int)=.{15,true}
	
	if value
		~assert(true)
	else
		~assert(false)
	
