module A
{
	Node :: struct {
		value: int;
	}	
}

module B
{
	Node :: struct {
		value: bool;
	}
	// Stacked modules
	module Maker
	{
		make :: () -> Node {
			result: Node;
			result.value = false;
			return result;
		}
	}
}

main :: ()
{
	a: A::Node;
	a.value = 5;
	
	b: B::Node;
	b = B::Maker::make();
	assert(b.value == false);
}