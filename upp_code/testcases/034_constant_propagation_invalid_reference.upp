Color :: enum
{
    RED;
    GREEN;
    BLUE;
}

main :: ()
{
    color: Color;
    &cast(*i32)*color = 0;
    
    // The reason that enums start with 1 is that
    // we want to error if they werent initialized correctly
    switch color
    {
        case Color.RED:
        case Color.GREEN:
        case Color.BLUE:
    }
}
